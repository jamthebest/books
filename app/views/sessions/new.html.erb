
<div class="autent">
<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :username %>
    <%= text_field_tag :username, params[:username] %>
  </div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  <div class="actions"><%= submit_tag "Log In" %>
<% end %>
</div>
</div>